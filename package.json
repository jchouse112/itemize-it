{
  "name": "itemize-it",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:web": "turbo dev --filter=@itemize-it/web",
    "dev:mobile": "turbo dev --filter=@itemize-it/mobile",
    "build:web": "turbo build --filter=@itemize-it/web"
  },
  "devDependencies": {
    "turbo": "^2.7.4",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "expo>react": "18.3.1",
      "expo-modules-core>react": "18.3.1",
      "react-native>react": "18.3.1",
      "react-native-reanimated>react": "18.3.1",
      "react-native-screens>react": "18.3.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "*",
        "react-dom": "*",
        "react-native": "0.76.9"
      },
      "ignoreMissing": [
        "react-dom"
      ]
    },
    "patchedDependencies": {
      "@expo/cli@0.22.27": "patches/@expo__cli@0.22.27.patch"
    }
  }
}
